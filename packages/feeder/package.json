{
  "name": "feeder",
  "version": "1.0.0",
  "description": "Feeds repository to the database",
  "main": "dist/server.js",
  "scripts": {
    "dev": "bun --watch src/server.ts",
    "dev:node": "nodemon src/server.ts",
    "build": "bun build src/server.ts --outdir ./dist --target node",
    "build:tsc": "tsc",
    "start": "bun dist/server.js",
    "start:node": "node dist/server.js",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "test": "bun test"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "morgan": "^1.10.0",
    "dotenv": "^16.3.1",
    "compression": "^1.7.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/morgan": "^1.9.9",
    "@types/compression": "^1.7.5",
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.54.0",
    "nodemon": "^3.0.2",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.0",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.8"
  },
  "keywords": [
    "express",
    "typescript",
    "backend",
    "api"
  ],
  "author": ""
}
